{
  "framework": "vite",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "cd dual-ai-chat && npm install && npm run build",
  "outputDirectory": "dual-ai-chat/dist"
}